{
  "Flex center": {
    "prefix": "flexcenter",
    "body": [
      "display: flex;",
      "justify-content: center;",
      "align-items: center;"
    ],
    "description": "Flex center"
  },
  "m 0 auto": {
    "prefix": "m0a",
    "body": [
      "margin: 0 auto;"
    ],
    "description": "m 0 auto"
  },
  "line-clamp": {
    "prefix": "line",
    "body": [
      "display: -webkit-box;",
      "-webkit-line-clamp: $1;",
      "-webkit-box-orient: vertical;",
      "overflow: hidden;"
    ],
    "description": "line-clamp"
  },
  "hide spin input": {
    "prefix": "hs",
    "body": [
      "input::-webkit-outer-spin-button,",
      "input::-webkit-inner-spin-button {",
      "  -webkit-appearance: none;",
      "  margin: 0;",
      "}"
    ],
    "description": "hide spin input"
  },
  "mediaqueryMin": {
    "prefix": "@min",
    "body": [
      "@media screen and (min-width:$1px){",
      "  $2",
      "}"
    ],
    "description": "mediaquery"
  },
  "mediaqueryMax": {
    "prefix": "@max",
    "body": [
      "@media screen and (max-width:$1px){",
      "  $2",
      "}"
    ],
    "description": "mediaquery"
  },
  "Absolute center": {
    "prefix": "absc",
    "body": [
      "position: absolute;",
      "top: 50%;",
      "left: 50%;",
      "transform: translate(-50%,-50%);"
    ],
    "description": "Absolute center"
  },
  "Absolute Top Left": {
    "prefix": "abstl",
    "body": [
      "position:absolute;",
      "top: $1;",
      "left: $2;"
    ],
    "description": "Position Absolute"
  },
  "Absolute Bottom Left": {
    "prefix": "absbl",
    "body": [
      "position:absolute;",
      "bottom: $1;",
      "left: $2;"
    ],
    "description": "Position Absolute"
  },
  "Absolute Top Right": {
    "prefix": "abstr",
    "body": [
      "position:absolute;",
      "top: $1;",
      "right: $2;"
    ],
    "description": "Position Absolute"
  },
  "Absolute Bottom Right": {
    "prefix": "absbr",
    "body": [
      "position:absolute;",
      "bottom: $1;",
      "right: $2;"
    ],
    "description": "Position Absolute"
  },
