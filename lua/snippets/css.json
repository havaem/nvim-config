  "Reset button css": {
    "prefix": "rsb",
    "body": [
      "border :none;",
      "outline: none;",
      "background: ${1:transparent};",
      "cursor: pointer;"
    ],
    "description": "Reset button css"
  },
  "Border outline none": {
    "prefix": "bon",
    "body": [
      "border :none;",
      "outline: none;"
    ],
    "description": "Border outline none"
  },
  "Reset Css": {
    "prefix": "resetcss",
    "body": [
      "html{box-sizing:border-box}*{margin:0;padding:0;list-style-type:none}*,:after,:before{box-sizing:border-box}button,input,select,textarea{outline:0}input{line-height:normal}button,label{cursor:pointer}a{text-decoration:none;color:#000}img{display:block;max-width:100%}"
    ],
    "description": "Reset Css"
  },
  "Custom scrollbar": {
    "prefix": "cs",
    "body": [
      "/* width */",
      "::-webkit-scrollbar {",
      "  width: 10px;",
      "}",
      "/* Track */",
      "::-webkit-scrollbar-track {",
      "  background: #f1f1f1;",
      "}",
      "/* Handle */",
      "::-webkit-scrollbar-thumb {",
      "  background: #888;",
      "}",
      "/* Handle on hover */",
      "::-webkit-scrollbar-thumb:hover {",
      "  background: #555;",
      "}"
    ],
    "description": "Custom scrollbar"
  },
  "Triangle top": {
    "prefix": "tritop",
    "body": [
      "width: 0px;",
      "height: 0px;",
      "border-left: ${1:5}px solid transparent;",
      "border-right: ${1:5}px solid transparent;",
      "border-bottom: ${1:5}px solid ${2:#000};"
    ],
    "description": "Triangle top"
  },
  "Triangle bottom": {
    "prefix": "tribottom",
    "body": [
      "width: 0px;",
      "height: 0px;",
      "border-left: ${1:5}px solid transparent;",
      "border-right: ${1:5}px solid transparent;",
      "border-top: ${1:5}px solid ${2:#000};"
    ],
    "description": "Triangle bottom"
  },
  "Triangle left": {
    "prefix": "trileft",
    "body": [
      "width: 0px;",
      "height: 0px;",
      "border-left: ${1:5}px solid transparent;",
      "border-right: ${1:5}px solid transparent;",
      "border-right: ${1:5}px solid ${2:#000};"
    ],
